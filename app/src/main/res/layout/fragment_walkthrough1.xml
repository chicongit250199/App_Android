<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
    android:background="@color/walkthough_background">


    <ImageView
        android:layout_width="370dp"
        android:layout_height="220dp"
        android:id="@+id/imageView4"
        android:contentDescription="@string/map_homepage"
        android:background="@drawable/map_homepage"
        android:layout_marginTop="100dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp" />

    <ImageView
        android:layout_width="160dp"
        android:layout_height="300dp"
        android:id="@+id/imageView5"
        android:layout_marginTop="41dp"
        android:background="@drawable/iphone6"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/backgroundbutton3"
        android:orientation="horizontal"
        android:layout_marginLeft="36dp"
        android:layout_marginStart="36dp"
        android:id="@+id/linearLayout3"
        android:layout_alignTop="@+id/imageView4"
        android:layout_alignLeft="@+id/imageView4"
        android:layout_alignStart="@+id/imageView4">

        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:id="@+id/imageView12"
            android:background="@drawable/call_transfer2"
            android:layout_margin="3dp"
            android:layout_gravity="center" />

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="Free video call for all"
            android:id="@+id/textView5"
            android:layout_margin="3dp"
            android:layout_gravity="center" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/backgroundbutton3"
        android:orientation="horizontal"
        android:id="@+id/linearLayout1"
        android:layout_marginTop="23dp"
        android:layout_below="@+id/linearLayout3"
        android:layout_alignLeft="@+id/imageView4"
        android:layout_alignStart="@+id/imageView4">

        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:id="@+id/conferenceImage"
            android:background="@drawable/conference2"
            android:layout_margin="3dp"
            android:layout_gravity="center" />

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="Conference call"
            android:id="@+id/textView51"
            android:layout_margin="3dp"
            android:layout_gravity="center" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/backgroundbutton3"
        android:orientation="horizontal"
        android:id="@+id/linearLayout12"
        android:layout_alignBottom="@+id/imageView4"
        android:layout_alignLeft="@+id/linearLayout3"
        android:layout_alignStart="@+id/linearLayout3"
        android:layout_marginBottom="23dp">

        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:id="@+id/conferenceImage12"
            android:background="@drawable/collaboration2"
            android:layout_margin="3dp"
            android:layout_gravity="center" />

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="Group messages"
            android:id="@+id/textView511"
            android:layout_margin="3dp"
            android:layout_gravity="center" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/backgroundbutton3"
        android:orientation="horizontal"
        android:id="@+id/linearLayout121"
        android:layout_alignTop="@+id/imageView4"
        android:layout_alignRight="@+id/imageView4"
        android:layout_alignEnd="@+id/imageView4"
        android:layout_marginTop="26dp">

        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:id="@+id/conferenceImage121"
            android:background="@drawable/multiple_devices2"
            android:layout_margin="3dp"
            android:layout_gravity="center" />

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="Multi-devices"
            android:id="@+id/textView5111"
            android:layout_margin="3dp"
            android:layout_gravity="center" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/backgroundbutton3"
        android:orientation="horizontal"
        android:id="@+id/linearLayout1211"
        android:layout_below="@+id/linearLayout1"
        android:layout_alignRight="@+id/imageView4"
        android:layout_alignEnd="@+id/imageView4">

        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:id="@+id/conferenceImage1211"
            android:background="@drawable/todo_list2"
            android:layout_margin="3dp"
            android:layout_gravity="center" />

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="Files management"
            android:id="@+id/textView51112"
            android:layout_margin="3dp"
            android:layout_gravity="center" />
    </LinearLayout>

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/imageView6"
        android:background="@drawable/logo"
        android:layout_above="@+id/linearLayout1211"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Antbuddy"
        android:id="@+id/textView6"
        android:textColor="@color/white_ab"
        android:textSize="15dp"
        android:gravity="center"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="130dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Team Communicates Happiness"
        android:id="@+id/textView10"
        android:layout_alignParentEnd="false"
        android:layout_alignParentStart="false"
        android:textColor="@color/white_ab"
        android:textSize="15dp"
        android:layout_alignParentLeft="false"
        android:gravity="center"
        android:layout_marginBottom="110dp"
        android:layout_alignParentBottom="true" />


</RelativeLayout>
